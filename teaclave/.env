TEACLAVE_BENCH_STRONG_SCALES=1,2,4,8,16,32
TEACLAVE_BENCH_INITIAL_SIZE=1024
TEACLAVE_BENCH_MAX_SIZE=65536
TEACLAVE_BENCH_TARGET_MS=0.5
TEACLAVE_BENCH_WARMUP=3
